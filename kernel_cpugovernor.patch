+static void darkside_early_suspend(struct early_suspend *handler) {
+        int i;
+        for_each_online_cpu(i)
+                darkside_suspend(i,1);
+}
+
+static void darkside_late_resume(struct early_suspend *handler) {
+        int i;
+        for_each_online_cpu(i)
+                darkside_suspend(i,0);
+}
